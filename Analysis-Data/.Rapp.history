N <- 100 #num sites#
G <- 5#
coef1 <- 3#
int <- -2#
set.seed(22)#
x <- runif(N)#
#
alpha_mu <- 0#
sigma <- 1#
set.seed(1)#
alpha <- rnorm(G, mean = alpha_mu, sd = sigma)#
grps <- rep(1:G, each = N/G)#
#
set.seed(50)#
y <- yprob <-  numeric(N)#
for (n in 1:N){#
  ymean <- int + (coef1*x[n]) + (alpha[grps[n]])#
  yprob[n] <- 1/(1+exp(-ymean))#
  y[n] <- rbinom(1, 1, yprob[n])#
}#
#
dat <- data.frame(y = y, x = x, grps =grps)
mb.stanarm <-mod.stanarm <- stan_glmer(y ~ x + (1|grps),#
                                                       data = dat,#
                                                       family = binomial(link = 'logit'),#
                                                       prior = t_prior,#
                                                       prior_intercept = t_prior,#
                                                       chains = 4, cores = 4, seed = 10)
summary(mb.stanarm)
